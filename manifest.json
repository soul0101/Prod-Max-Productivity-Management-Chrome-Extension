{
    "manifest_version": 2,
    "name": "Productivity Manager",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "productivity.png"
    },    
    "permissions": ["<all_urls>","storage","activeTab","notifications","webRequest", "webRequestBlocking"]
}